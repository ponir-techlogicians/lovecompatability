msgstr "1קמ"

msgid "50km"
msgstr "50km"

msgid "200km"
msgstr "200km"

msgid "world"
msgstr "עולם"

msgid "Rank"
msgstr "דירוג"

msgid "Mentioned (times)"
msgstr "הוזכר (פעמים)"

msgid "With who?"
msgstr "עם מי?"
